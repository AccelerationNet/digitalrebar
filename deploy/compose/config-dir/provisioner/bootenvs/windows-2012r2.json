{
    "Name": "windows-2012r2-install",
    "TenantId": 1,
    "OS": {
        "Name": "windows-2012r2",
        "IsoFile": "rebar-win2012r2.iso",
        "IsoSha256": "7e413b107d80cc0f16e289e5c1aba22751eb01d713de48a5cf08f7f35fdc8e12"
    },
    "Kernel": "wimboot",
    "Initrds": [ "boot/bcd", "boot/boot.sdi", "rebar-winpe.wim" ],
    "BootParams": "pause",
    "RequiredParams": [
        "logging_servers",
        "ntp_servers",
        "operating-system-disk",
        "provisioner-default-password-hash",
        "proxy-servers",
        "rebar-access_keys",
        "rebar-machine_key"
    ],
    "Templates": [
        {
            "Name": "pxelinux",
            "Path": "pxelinux.cfg/{{.Machine.HexAddress}}",
            "UUID": "windows.pxelinux.tmpl"
        },
        {
            "Name": "ipxe",
            "Path": "{{.Machine.Address}}.ipxe",
            "UUID": "windows.ipxe.tmpl"
        }
    ]
}
