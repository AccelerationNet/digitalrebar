{
    "bios": {
	"set_mapping": [
	    {
		"match": {
		    "system_manufacturer": "Dell Inc.",
		    "system_product": {
			"__sm_leaf": true,
			"op": "regex",
			"match": "^PowerEdge (R.|T[34])20"
		    }
		},
		"role": "bios-dell-rseries-configure",
		"configs": [
		    {
			"name": "default",
			"settings": {
			    "AcPwrRcvry": "Last",
			    "AcPwrRcvryDelay": "Immediate",
			    "AcPwrRcvryUserDelay": "60",
			    "AesNi": "Enabled",
			    "BootMode": "Bios",
			    "BootSeqRetry": "Enabled",
			    "ConTermType": "Vt100Vt220",
			    "CorePerfBoost": "Enabled",
			    "DataReuse": "Enabled",
			    "DcuIpPrefetcher": "Enabled",
			    "DcuStreamerPrefetcher": "Enabled",
			    "DmaVirtualization": "Enabled",
			    "EmbNic1Nic2": "Enabled",
			    "EmbNic1": "EnabledPxe",
			    "EmbNic2": "EnabledPxe",
			    "EmbNic3Nic4": "Enabled",
			    "EmbNic3": "EnabledPxe",
			    "EmbNic4": "EnabledPxe",
			    "EmbSata": "AhciMode",
			    "EmbVideo": "Enabled",
			    "ErrPrompt": "Disabled",
			    "eSataPort1": "Auto",
			    "ExtSerialConnector": "RemoteAccDevice",
			    "FailSafeBaud": "115200",
			    "InSystemCharacterization": "Enabled",
			    "IntegratedNetwork1": "Enabled",
			    "IntegratedNetwork2": "Enabled",
			    "IntegratedRaid": "Enabled",
			    "IntegratedSas": "Enabled",
			    "IntelTxt": "Off",
			    "InternalSdCard": "Off",
			    "InternalSdCardRedundancy": "Mirror",
			    "InternalUsb": "On",
			    "InternalUsb1": "On",
			    "InternalUsb2": "On",
			    "LogicalProc": "Enabled",
			    "MemFrequency": "MaxPerf",
			    "MemOpMode": "OptimizerMode",
			    "MemPatrolScrub": "Standard",
			    "MemPwrMgmt": "Enabled",
			    "MemRefreshRate": "1x",
			    "MemTest": "Enabled",
			    "NmiButton": "Disabled",
			    "NodeInterleave": "Disabled",
			    "NumLock": "On",
			    "OsWatchdogTimer": "Disabled",
			    "PowerDelivery": "MaxReliability",
			    "ProcAdjCacheLine": "Enabled",
			    "ProcBusSpeed": "7.20 GT/s",
			    "ProcC1E": "Enabled",
			    "ProcCStates": "Enabled",
			    "ProcCores": "All",
			    "ProcDramPrefetcher": "Enabled",
			    "ProcExecuteDisable": "Enabled",
			    "ProcHtAssist": "Enabled",
			    "ProcHwPrefetcher": "Enabled",
			    "ProcHyperTransport": "HT3",
			    "ProcPwrPerf": "SysDbpm",
			    "ProcTurboMode": "Enabled",
			    "ProcVirtualization": "Disabled",
			    "PwrButton": "Enabled",
			    "QpiSpeed": "MaxDataRate",
			    "RedirAfterBoot": "Enabled",
			    "ReportKbdErr": "Report",
			    "RtidSetting": "Disabled",
			    "SataPortA": "Auto",
			    "SataPortB": "Auto",
			    "SataPortC": "Auto",
			    "SataPortD": "Auto",
			    "SataPortE": "Auto",
			    "SataPortF": "Auto",
			    "SataPortG": "Auto",
			    "SataPortH": "Auto",
			    "SerialComm": "OnConRedirCom2",
			    "SerialPortAddress": "Serial1Com1Serial2Com2",
			    "Slot1": "Enabled",
			    "Slot2": "Enabled",
			    "Slot3": "Enabled",
			    "Slot4": "Enabled",
			    "Slot5": "Enabled",
			    "Slot6": "Enabled",
			    "Slot7": "Enabled",
